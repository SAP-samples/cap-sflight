{
  "name": "travel-processor",
  "private": true,
  "main": "webapp/index.html",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "build": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo",
    "start": "ui5 serve",
    "test": "npm run test:node && npm run test:java",
    "test:java": "karma start --server=java",
    "test:node": "karma start --server=node"
  },
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "devDependencies": {
    "@ui5/cli": "^2.14.17",
    "cross-spawn": "^7.0.3",
    "http-proxy": "^1.18.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-ui5": "^2.4.0",
    "puppeteer": "^19.5.0",
    "ui5-middleware-simpleproxy": "^0.9.2",
    "ui5-task-zipper": "^0.7.1"
  },
  "ui5": {
    "dependencies": [
      "ui5-task-zipper",
      "ui5-middleware-simpleproxy"
    ]
  }
}
