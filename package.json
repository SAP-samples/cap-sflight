{
  "name": "@capire/sflight",
  "version": "1.0.0",
  "description": "CAP flight demo scenario",
  "repository": "https://github.com/SAP-samples/cap-sflight",
  "dependencies": {
    "@sap/audit-logging": "^5.0.0",
    "@sap/cds": "^5.1",
    "@sap/hana-client": "^2.8.20",
    "@sap/xsenv": "^3.1.0",
    "@sap/xssec": "^3.2.0",
    "express": "^4",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.84",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "jest": "^26.6.3",
    "qunit": "^2.9.3",
    "sqlite3": "^5.0.2"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "cds run",
    "test": "jest --runInBand"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[development]": {
          "strategy": "dummy"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "log": {
      "service": true
    },
    "features": {
      "preserve_computed": true
    }
  },
  "sapux": [
    "app/travel_processor"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "license": "SAP SAMPLE CODE LICENSE",
  "private": true
}
