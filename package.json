{
  "name": "@capire/sflight",
  "version": "1.0.0",
  "private": true,
  "description": "CAP flight demo scenario",
  "license": "SAP SAMPLE CODE LICENSE",
  "repository": "https://github.com/SAP-samples/cap-sflight",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "lint": "npx eslint .",
    "start": "cds run",
    "test": "jest",
    "test:mocha": "npx mocha --timeout 20000"
  },
  "dependencies": {
    "@sap/audit-logging": "^5.0.0",
    "@sap/cds": "^5.4.5",
    "@sap/xssec": "^3.2.7",
    "express": "^4",
    "hdb": "^0.18.3",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.90.12",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "jest": "^27.1.0",
    "qunit": "^2.9.3",
    "sqlite3": "^5.0.2"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sql"
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[development]": {
          "strategy": "dummy"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "log": {
      "service": true
    },
    "features": {
      "preserve_computed": true
    },
    "build": {
      "[node]": {
        "target": "gen",
        "tasks": [{"for": "node-cf"}, {"for": "hana"}]
      }
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 20000
  },
  "sapux": [
    "app/travel_processor"
  ]
}
