{
  "name": "@capire/sflight",
  "version": "1.0.0",
  "private": true,
  "description": "CAP flight demo scenario",
  "license": "SAP SAMPLE CODE LICENSE",
  "repository": "https://github.com/SAP-samples/cap-sflight",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint": "npx eslint .",
    "start": "cds-serve",
    "test": "jest",
    "test:mocha": "npx mocha --timeout 20000"
  },
  "dependencies": {
    "@sap/cds": ">=7.0.0",
    "@sap/cds-hana": "^2.0.0",
    "@sap/xssec": "^3.2.7",
    "express": "^4"
  },
  "devDependencies": {
    "@ui5/cli": "^3.3.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-ui5": "^3.0.3",
    "puppeteer": "^21.0.1",
    "ui5-middleware-simpleproxy": "^3.0.0",
    "ui5-task-zipper": "^3.0.0",
    "@cap-js/sqlite": "^1.0.1",
    "axios": "^1",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0"
  },
  "cds": {
    "requires": {
      "[development]": {
        "auth": {
          "users": {
            "alice": {
              "roles": [
                "admin", "reviewer"
              ]
            },
            "bob": {
              "roles": [
                "processor"
              ]
            },
            "carol": {
              "roles": []
            }
          }
        }
      }
    },
    "log": {
      "service": true
    },
    "features": {
      "odata_new_parser": true
    },
    "build": {
      "[node]": {
        "target": "gen",
        "tasks": [
          {
            "for": "node-cf"
          },
          {
            "for": "hana"
          }
        ]
      },
      "[java]": {
        "target": ".",
        "tasks": [
          {
            "for": "java-cf"
          }
        ]
      },
      "[hana]": {
        "target": "gen",
        "tasks": [
          {
            "for": "hana"
          }
        ]
      }
    }
  },
  "jest": {
    "testTimeout": 20000
  }
}
